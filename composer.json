    "form",
    "select list"
  ],
  "homepage": "http://www.sklein-medien.de",
  "authors": [
    {
      "name": "Sebastian Klein",
      "email": "sebastian@sklein-medien.de"
    }
  ],
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "require": {
